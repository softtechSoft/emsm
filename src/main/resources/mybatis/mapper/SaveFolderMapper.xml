<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
          PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
          "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.softtech.mappers.SaveFolderMapper">

	<select id="findFileTypeCode" parameterType="String"
		resultType="com.softtech.entity.SaveFolder">
		SELECT *
		FROM m_file
		WHERE fileTypeCode =#{fileTypeCode}
	</select>


</mapper>
